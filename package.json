{
  "name": "meeting_buddy_pro",
  "version": "0.1.0",
  "description": "Meeting Buddy Pro desktop app with multi-speaker diarized transcription",
  "private": true,
  "main": "electron/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm:dev:backend\" \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "dev:backend": "nodemon backend/server.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production electron ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "electron",
    "meeting",
    "diarization"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^38.4.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.12",
    "wait-on": "^9.0.1"
  }
}
